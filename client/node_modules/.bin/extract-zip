#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hydra/Documents/ProjetMobile/client/node_modules/extract-zip/node_modules:/home/hydra/Documents/ProjetMobile/client/node_modules:/home/hydra/Documents/ProjetMobile/node_modules:/home/hydra/Documents/node_modules:/home/hydra/node_modules:/home/node_modules:/node_modules:/home/hydra/Documents/ProjetMobile/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hydra/Documents/ProjetMobile/client/node_modules/extract-zip/node_modules:/home/hydra/Documents/ProjetMobile/client/node_modules:/home/hydra/Documents/ProjetMobile/node_modules:/home/hydra/Documents/node_modules:/home/hydra/node_modules:/home/node_modules:/node_modules:/home/hydra/Documents/ProjetMobile/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../extract-zip/cli.js" "$@"
else
  exec node  "$basedir/../extract-zip/cli.js" "$@"
fi
